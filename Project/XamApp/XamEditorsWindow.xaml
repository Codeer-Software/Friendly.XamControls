<Window x:Class="XamApp.XamEditorsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ig="http://schemas.infragistics.com/xaml" xmlns:Custom="http://infragistics.com/DataPresenter" xmlns:Custom1="http://infragistics.com/OutlookBar" xmlns:Custom2="http://infragistics.com/DockManager" xmlns:Custom3="http://infragistics.com/Editors"
        Title="XamEditorsWindow" Height="300" Width="300">
    <Grid>

        <Custom3:XamComboEditor x:Name="_combo" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.172,-1.727" Height="23" Width="112"/>

        <Custom3:XamNumericEditor x:Name="_numeric" HorizontalAlignment="Left" Margin="10,51,0,0" VerticalAlignment="Top" Height="23" Width="112"/>
        <Custom3:XamDateTimeEditor x:Name="_dateTime" HorizontalAlignment="Left" Margin="10,93,0,0" VerticalAlignment="Top" Width="112"/>
        <Custom3:XamCurrencyEditor x:Name="_currency" HorizontalAlignment="Left" Margin="10,135,0,0" VerticalAlignment="Top" Width="112"/>
        <Custom3:XamMaskedEditor x:Name="_masked" HorizontalAlignment="Left" Margin="10,173,0,0" VerticalAlignment="Top" Width="112"/>
        <Custom3:XamTextEditor x:Name="_text" HorizontalAlignment="Left" Margin="10,213,0,0" VerticalAlignment="Top" Width="112"/>

    </Grid>
</Window>
